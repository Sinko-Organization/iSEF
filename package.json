{
  "name": "isef",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.13.4",
  "scripts": {
    "typecheck": "tsc",
    "build": "next build",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "studio": "prisma studio",
    "format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "format:check": "prettier --check --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-path ../.gitignore",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore . --fix",
    "lint": "next lint",
    "start": "next start",
    "test": "vitest run",
    "test:unit": "vitest run unit",
    "test:integration": "vitest run int",
    "test:watch": "vitest",
    "commit": "git add . && cz",
    "prepare": "husky install",
    "cypress:open": "cypress open"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    },
    "commitizenEmoji": {
      "conventionalFormat": true
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.4.0",
    "@trpc/client": "^9.27.2",
    "@trpc/next": "^9.27.2",
    "@trpc/react": "^9.27.2",
    "@trpc/server": "^9.27.2",
    "immer": "^9.0.16",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "next-auth": "^4.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
    "react-query": "^3.39.2",
    "react-spreadsheet-import": "^2.0.8",
    "superjson": "^1.10.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@next/eslint-plugin-next": "^13.0.1",
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "commitizen-emoji": "^1.0.5",
    "commitlint": "^17.1.2",
    "commitlint-config-gitmoji": "^2.2.6",
    "cypress": "^10.10.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chakra-ui": "^0.7.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "eslint-plugin-vitest": "^0.0.11",
    "husky": "^8.0.0",
    "jsdom": "^20.0.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4",
    "vite": "^3.1.7",
    "vitest": "^0.24.1"
  },
  "ct3aMetadata": {
    "initVersion": "5.13.1"
  },
  "peerDependencies": {
    "commitlint": "^11.0.0"
  }
